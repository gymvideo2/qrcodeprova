<?xml version='1.0' encoding='utf-8'?>
<widget id="com.qr.two" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="QRScanner">
        <param name="browser-package" value="QRScanner" />
    </feature>
    <name>qrcodeprova</name>
    <description>
App Ufficiale di Rocca delle Caminate.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        Giammarco Tassinari
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="cli-7.0.1" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="BackgroundColor" value="0x00000000" />
</widget>
